<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.spierce.dibench">

	<application android:name=".DiBenchmarkApplication"
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">

		<activity android:name=".MainActivity"
			android:label="Benchmark Home">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity android:name=".Dagger2BenchmarkActivity"
			android:label="Dagger 2 Benchmark" />
		<activity android:name=".KodeinBenchmarkActivity"
			android:label="Kodein Benchmark" />
	</application>

</manifest>