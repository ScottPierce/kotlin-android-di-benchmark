<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:padding="16dp"
	android:gravity="center_horizontal"
	tools:context="com.spierce.dibench.MainActivity">

	<TextView android:id="@+id/data"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		/>

	<Button android:id="@+id/reInject"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Re-Inject"
		/>

	<Button android:id="@+id/dagger2Benchmark"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Run Dagger 2 Benchmark"
		android:layout_gravity="bottom"
		/>

	<Button android:id="@+id/kodeinBenchmark"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Run Kodein Benchmark"
		android:layout_gravity="bottom"
		/>

	<Button android:id="@+id/kodeinErasedBenchmark"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Run Kodein Erased Benchmark"
		android:layout_gravity="bottom"
		/>

	<Button android:id="@+id/kinjectBenchmark"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Run Kinject Benchmark"
		android:layout_gravity="bottom"
		/>

</LinearLayout>
